<configuration>
    <appender name="A1" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d] [%t] %-5p %c{5} - %m%n</pattern>
        </encoder>
    </appender>
  
    <logger name="p6spy" level="TRACE"/>
    <logger name="com.serphacker" level="TRACE"/>
    <logger name="com.serphacker.serposcope.scraper.http.extensions" level="ERROR" />
    <root level="WARN">
        <appender-ref ref="A1"/>
    </root>
</configuration>
